{
    "scenario_geminifiles_test": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "gemini-cli": {
                "geminifiles": "https://github.com/marblejenka/geminifiles.git"
            }
        }
    },
    "scenario_node_user": {
        "image": "mcr.microsoft.com/devcontainers/javascript-node:20",
